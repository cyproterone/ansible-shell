<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>RunAtLoad</key> <true/>
    <key>StartInterval</key> <integer>86400</integer>
    <key>Label</key> <string>bing-image</string>
    <key>StandardOutPath</key> <string>{{ ansible_env.HOME }}/.cache/bing.stdout</string>
    <key>StandardErrorPath</key> <string>{{ ansible_env.HOME }}/.cache/bing.stderr</string>
    <key>ProgramArguments</key>
    <array>
      <string>{{ ansible_env.HOME }}/.config/scripts/_bing</string>
      <string>-o</string>
      <string>{{ ansible_env.HOME }}/.icloud/桌面背景</string>
      <string>-d</string>
      <string>7</string>
    </array>
  </dict>
</plist>
