alias v='vagrant'
alias vdun='vagrant destroy'

#################### ########### ####################
#################### VBox Region ####################
#################### ########### ####################

alias vb='VBoxManage'

vboff() {
  vb controlvm $1 poweroff
}

vbdun() {
  vboff $1
  vb unregistervm $1
}
